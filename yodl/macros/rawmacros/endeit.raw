<STARTDOC>
macro(endeit())
(DEPRECATED. Use NOEXPAND(enumeration()).)
<>

DEFINEMACRO(endeit)(0)(\
<html>
    NOTRANS(</ol>)\
<latex>
    NOTRANS(\end{enumerate})\
<man>
    POPCOUNTER(XXenumcounter)\
    ADDTOCOUNTER(XXinlist)(-1)\
<sgml>
    NOTRANS(</enumerate>)\
<texinfo>
    NOTRANS(@end enumerate)\
<txt>
    POPCOUNTER(XXenumcounter)\
<xml>
    XXmlEndList(XXenumcounter)(li)\
    NOTRANS(</ol>)\
    POPCOUNTER(XXenumcounter)\
<>
)

